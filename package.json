{
  "name": "coreui-table",
  "version": "1.1.7",
  "description": "CoreUI table",
  "scripts": {
    "build_all": "gulp build_tpl && gulp build_js && gulp build_js_min && gulp build_css && gulp build_css_min && npm run test_js",

    "build_tpl": "gulp build_tpl",
    "build_js": "gulp build_tpl && gulp build_js",
    "build_js_min": "gulp build_tpl && gulp build_js_min",
    "build_css": "gulp build_css",
    "build_css_min": "gulp build_css_min",

    "build_watch": "gulp build_watch",
    "build_bootstrap": "gulp build_bootstrap",
    "test_js": "eslint -c .eslintrc.json --max-warnings 100 --ext .js ./dist"
  },
  "dependencies": {
    "ejs": "3.1.*"
  },
  "devDependencies": {
    "gulp": "4.*",
    "gulp-concat": "2.*",
    "gulp-sourcemaps": "3.*",
    "gulp-uglify": "3.0.*",
    "gulp-babel": "8.0.0",
    "gulp-sass": "5.1.0",
    "gulp-sort": "2.0.0",
    "gulp-wrap-file": "0.1.2",
    "gulp-html-to-js": "0.0.7",
    "rollup-stream": "1.24.1",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-sourcemaps": "0.6.3",
    "vinyl-source-stream": "2.0.0",
    "vinyl-buffer": "1.0.1",
    "sass": "1.68.0",
    "eslint": "8.50.0",
    "bootstrap": "5.3.3",
    "@babel/plugin-transform-template-literals": "7.22.5",
    "@babel/preset-env": "7.23.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/n2ref/coreui-table.git"
  },
  "keywords": [
    "js",
    "frontend",
    "framework",
    "table",
    "bootstrap"
  ],
  "author": "n2ref",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/n2ref/coreui-table/issues"
  },
  "homepage": "https://github.com/n2ref/coreui-table#readme"
}
